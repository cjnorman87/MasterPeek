# platform :ios, '9.0'

target 'Peek' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Peek
   pod 'Firebase/Core'
   pod 'Firebase/Firestore'
   pod 'Firebase/Database'
   pod 'Firebase/Auth'
   pod 'Firebase/Storage'
   pod 'Alamofire', '~> 4.7'
  target 'PeekTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PeekUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
